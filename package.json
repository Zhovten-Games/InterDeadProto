{
  "name": "interdead-proto",
  "version": "0.1.0",
  "private": true,
  "description": "Build and formatting pipeline for InterDeadProto.",
  "scripts": {
    "dev": "vite --config vite.config.js",
    "build": "vite build --config vite.config.js",
    "format": "prettier --write \"proto-dev/src/**/*.{js,json}\" \"proto-dev/tests/**/*.js\" \"proto-dev/index.html\" \"*.{md,js,json}\" \".github/workflows/*.yml\"",
    "format:check": "prettier --check \"proto-dev/src/**/*.{js,json}\" \"proto-dev/tests/**/*.js\" \"proto-dev/index.html\" \"*.{md,js,json}\" \".github/workflows/*.yml\""
  },
  "devDependencies": {
    "prettier": "^3.3.0",
    "vite": "^5.4.11"
  }
}
